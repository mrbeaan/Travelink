<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="travelink.mobile.travelink">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_activity" />
        <activity android:name=".Main_activity" />
        <activity android:name=".Booking.Booking_activity" />
        <activity android:name=".Booking.Schedule_activity" />
        <activity android:name=".Booking.Detail_book_activity" />
        <activity android:name=".History.History_activity" />
        <activity android:name=".Account.Account_activity" />
        <activity android:name=".Setting.Setting_activity" />
        <activity android:name=".Register_activity" />
        <activity android:name=".Countdown_activity"></activity>
    </application>

</manifest>